<?xml-stylesheet href="changepassword.css"?>


<tns:WebPage xmlns:tns="https://interdark.com/ChangePasswordPage/changepassword.xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://interdark.com/ChangePasswordPage/changepassword.xml changepassword.xsd">
	<tns:headPage>
		<tns:titleText>InterDark | Change password</tns:titleText>
	</tns:headPage>
	
	<tns:bodyPage>
		<tns:topMenuBar>
			<tns:firstMenuItem>
				<tns:firstMenuItemName>My Account</tns:firstMenuItemName>
				<tns:firstdropitem>Home</tns:firstdropitem>
				<tns:seconddropitem>Change Password</tns:seconddropitem>
				<tns:thirddropitem>Change Email</tns:thirddropitem>
			</tns:firstMenuItem>
	        <tns:secondMenuItem>Logout</tns:secondMenuItem>
